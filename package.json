{
    "name": "tox-time",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "nodemon --config ./config.json ./server.js",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "vue-cli-service build --mode development --watch",
        "dev:backend": "webpack --progress --mode development --watch --config webpack.config.server.js",
        "dev:serve": "nodemon --config ./config.json -L --inspect=0.0.0.0:9001 ./server.js",
        "build": "webpack --progress --mode production --config webpack.config.server.js && vue-cli-service build --no-unsafe-inline",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "app-root-path": "^3.0.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "concurrently": "^5.3.0",
