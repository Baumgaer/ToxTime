{
    "name": "tox-time",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "nodemon --config ./config.json ./server.js",
        "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
        "dev:frontend": "vue-cli-service build ./src/client/main.js --mode development --watch",
        "dev:backend": "webpack --progress --mode development --watch --config webpack.config.server.js",
        "dev:serve": "nodemon --config ./config.json -L --inspect=0.0.0.0:9001 ./server.js",
        "build": "webpack --progress --mode production --config webpack.config.server.js && vue-cli-service build --no-unsafe-inline ./src/client/main.js",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "app-root-path": "^3.0.0",
        "compression": "^1.7.4",
        "concurrently": "^5.3.0",
        "connect-mongodb-session": "^2.4.1",
        "core-js": "^3.6.5",
        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "helmet": "^4.2.0",
        "hpp": "^0.2.3",
        "lodash": "^4.17.20",
        "mongoose": "^5.11.8",
        "ms": "^2.1.3",
        "uuid": "^8.3.2",
        "vue": "^2.6.11",
        "vue-i18n": "^8.22.2",
        "vue-router": "^3.4.9"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/plugin-proposal-optional-chaining": "^7.12.7",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/runtime": "^7.12.5",
        "@types/vue": "^2.0.0",
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "cache-loader": "^4.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "jsconfig-paths-webpack-plugin": "^0.1.3",
        "less": "^3.12.2",
        "less-loader": "^7.1.0",
        "nodemon": "^2.0.6",
        "terser-webpack-plugin": "^4.2.3",
        "thread-loader": "^3.0.1",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.44.2",
        "webpack-cli": "^3.3.12",
        "webpack-node-externals": "^2.5.2"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
