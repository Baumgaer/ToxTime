        "express": "^4.17.1",
        "express-session": "^1.17.1",
        "helmet": "^4.3.1",
        "hpp": "^0.2.3",
        "http-errors": "^1.8.0",
        "lodash": "^4.17.20",
        "mongoose": "^5.11.8",
        "ms": "^2.1.3",
        "nodemailer": "^6.4.17",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-local-mongoose": "^6.0.1",
        "source-map-support": "^0.5.19",
